MapCity.Widgets.AdvancedSearchComboBox = MapCity.Core.extend(Object,{
	constructor: function(options){
		this.options = {
			street: null
			,number: null
			,intersection: null
			,district: null
			,action: null
			,actionCallback: null
			,version: MapCity.Config.versionWS
